{
  "kind": "collectionType",
  "collectionName": "articles",
  "info": {
    "singularName": "article",
    "pluralName": "articles",
    "displayName": "Article",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Item": {
      "type": "component",
      "repeatable": true,
      "component": "article-components.item"
    },
    "Title": {
      "type": "string"
    },
    "sites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::site.site"
    },
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "primary_site": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::site.site"
    },
    "Author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    }
  }
}
